{
	"name": "new_blank_pixijs_app",
	"version": "0.0.0-0.0",
	"description": "pixijs app",
	"private": true,
	"scripts": {
		"build": "webpack --node-env LOCAL --bail --progress --profile --config webpack_config/webpack.dev.js",
		"build-dev": "webpack --node-env DEVELOPMENT --bail --progress --profile --config webpack_config/webpack.dev.js",
		"build-qa": "webpack --node-env QA --bail --progress --profile --config webpack_config/webpack.dev.js",
		"build-stage": "webpack --node-env STAGE --bail --progress --profile --config webpack_config/webpack.prod.js",
		"build-prod": "webpack --node-env PRODUCTION --bail --progress --profile --config webpack_config/webpack.prod.js",
		"build-sound": "webpack --config webpack_config/webpack.sound.js --action=build",
		"rename-sound": "webpack --config webpack_config/webpack.sound.js --action=rename",
		"start": "webpack serve --node-env DEVELOPMENT --config webpack_config/webpack.dev.js --progress --open",
		"lint": "eslint --ext .js,.ts .",
		"prettier": "prettier \"./**/*.+(js|json|ts|html|scss|css)\"",
		"format": "npm run prettier -- --write",
		"check-format": "npm run prettier -- --check",
		"validate": "npm-run-all --parallel lint check-format"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
